{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cricketereweb": {
      "root": "",
      "sourceRoot": "src",
      "schematics": {
        "@schematics/angular:component": {
          "style": "less"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "projectType": "application",
      "prefix": "cr",
      "architect": {
        "writeindex": {
          "builder": "./builder:localizeIndex",
          "options": {
            "source": "host/client/placeholder.html",
            "destination": "host/index",
            "isUrlBased": true,
            "prefix": "cr",
            "languages": [
              {
                "name": "ar",
                "isRtl": true
              },
              {
                "name": "en",
                "isRtl": false
              },
              {
                "name": "fr",
                "isRtl": false
              }
            ]
          }
        },
        "extract": {
          "builder": "./builder:extract",
          "options": {
            "destination": "./src/locale",
            "scan": "./src/app/components",
            "languages": [
              {
                "name": "en",
                "localeId": "en"
              },
              {
                "name": "ar",
                "localeId": "ar-JO",
                "isDefault": true
              },
              {
                "name": "fr",
                "localeId": "fr-CA"
              }
            ]
          }
        },
        "writeindex-netlify": {
          "builder": "./builder:localizeIndex",
          "options": {
            "source": "netlify/client/placeholder.html",
            "destination": "netlify/client",
            "languages": [
              {
                "name": "ar",
                "isRtl": true
              },
              {
                "name": "en",
                "isRtl": false
              },
              {
                "name": "fr",
                "isRtl": false
              }
            ]
          }
        },
        "writeindex-firebase": {
          "builder": "./builder:localizeIndex",
          "options": {
            "source": "firebase/client/placeholder.html",
            "destination": "firebase/client",
            "languages": [
              {
                "name": "ar",
                "isRtl": true
              },
              {
                "name": "en",
                "isRtl": false
              },
              {
                "name": "fr",
                "isRtl": false
              }
            ]
          }
        },
        "writeindex-firebase-i18n": {
          "builder": "./builder:localizeIndex",
          "options": {
            "source": "firebase/client/placeholder.html",
            "destination": "firebase/client",
            "withFolders": true,
            "languages": [
              {
                "name": "ar",
                "isRtl": true
              },
              {
                "name": "en",
                "isRtl": false
              },
              {
                "name": "fr",
                "isRtl": false
              }
            ]
          }
        },
        "writeindex-surge": {
          "builder": "./builder:localizeIndex",
          "options": {
            "source": "surge/client/placeholder.html",
            "destination": "surge/client",
            "withFolders": true,
            "fileName": "200.html",
            "languages": [
              {
                "name": "ar",
                "isRtl": true
              },
              {
                "name": "en",
                "isRtl": false
              },
              {
                "name": "fr",
                "isRtl": false
              }
            ]
          }
        },
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "resourcesOutputPath": "assets/",

            "index": "src/placeholder.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "styles": [
              {
                "input": "src/assets/css/cricketere.css",
                "bundleName": "styles.ltr",
                "inject": false
              },
              {
                "input": "src/assets/css/cricketere.rtl.css",
                "bundleName": "styles.rtl",
                "inject": false
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "./mockup/less",
                "./node_modules/shut/src/less"
              ]
            },
            "scripts": [
              {
                "input": "src/assets/js/cricketere.js",
                "bundleName": "cricketere"
              }
            ],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "images/**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "*",
                "input": "configs",
                "output": "/localdata"
              },
              {
                "glob": "*.js",
                "input": "src/locale",
                "output": "/locale"
              },
              {
                "glob": "*(ar-JO|en).js",
                "input": "node_modules/@angular/common/locales/global",
                "output": "/locale"
              }
            ]
          },
          "configurations": {
            "serve": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "commonChunk": true,
              "aot": true,
              "index": "src/index.dev.html",
              "styles": [
                {
                  "input": "src/assets/css/cricketere.css",
                  "bundleName": "styles"
                }
              ]
            },
            "serveProduction": {
              "namedChunks": true,
              "vendorChunk": false,
              "commonChunk": true,
              "optimization": false,
              "aot": true,
              "sourceMap": true,
              "index": "src/index.dev.prod.html",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "serveRtl": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "commonChunk": true,
              "aot": true,
              "index": "src/index.rtl.html",
              "styles": [
                {
                  "input": "src/assets/css/cricketere.rtl.css",
                  "bundleName": "styles"
                }
              ]
            },
            "production": {
              "outputPath": "./host/client/",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": {
                "scripts": true,
                "fonts": true,
                "styles": {
                  "inlineCritical": true,
                  "minify": true
                }
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "deleteOutputPath": true,
              "assets": [
                {
                  "glob": "favicon.ico",
                  "input": "src",
                  "output": "/"
                },
                {
                  "glob": "images/**/*",
                  "input": "src/assets",
                  "output": "/assets"
                },
                {
                  "glob": "*(cr-en|cr-ar).js",
                  "input": "src/locale",
                  "output": "/locale"
                },
                {
                  "glob": "*(ar-JO|en).js",
                  "input": "node_modules/@angular/common/locales/global",
                  "output": "/locale"
                }
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4mb"
                }
              ]
            },
            "netlify": {
              "outputPath": "./netlify/client/",
              "assets": [
                {
                  "glob": "favicon.ico",
                  "input": "src",
                  "output": "/"
                },
                {
                  "glob": "config.prod.json",
                  "input": "netlify",
                  "output": "/localdata"
                },
                {
                  "glob": "images/**/*",
                  "input": "src/assets",
                  "output": "/assets"
                },
                {
                  "glob": "*(cr-en|cr-ar).js",
                  "input": "src/locale",
                  "output": "/locale"
                },
                {
                  "glob": "*(ar-JO|en).js",
                  "input": "node_modules/@angular/common/locales/global",
                  "output": "/locale"
                },
                {
                  "glob": "404.html",
                  "input": "netlify",
                  "output": "/"
                }
              ]
            },
            "netlifyUrl": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/app/app.module.ts",
                  "with": "src/app/app.prefix.module.ts"
                }
              ]
            },
            "firebase": {
              "baseHref": "/",
              "outputPath": "./firebase/client/",
              "assets": [
                {
                  "glob": "favicon.ico",
                  "input": "src",
                  "output": "/"
                },
                {
                  "glob": "images/**/*",
                  "input": "src/assets",
                  "output": "/assets"
                },
                {
                  "glob": "*(cr-en|cr-ar).js",
                  "input": "src/locale",
                  "output": "/locale"
                },
                {
                  "glob": "*(ar-JO|en).js",
                  "input": "node_modules/@angular/common/locales/global",
                  "output": "/locale"
                },
                {
                  "glob": "config.prod.json",
                  "input": "firebase",
                  "output": "/localdata"
                },
                {
                  "glob": "index.html",
                  "input": "firebase",
                  "output": "/"
                }
              ]
            },
            "firebaseUrl": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/app/app.module.ts",
                  "with": "src/app/app.prefix.module.ts"
                }
              ]
            },
            "surge": {
              "outputPath": "./surge/client",
              "baseHref": "/",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/app/app.module.ts",
                  "with": "src/app/app.prefix.module.ts"
                }
              ],
              "assets": [
                {
                  "glob": "favicon.ico",
                  "input": "src",
                  "output": "/"
                },
                {
                  "glob": "config.prod.json",
                  "input": "surge",
                  "output": "/localdata"
                },
                {
                  "glob": "images/**/*",
                  "input": "src/assets",
                  "output": "/assets"
                },
                {
                  "glob": "*(cr-en|cr-ar).js",
                  "input": "src/locale",
                  "output": "/locale"
                },
                {
                  "glob": "*(ar-JO|en).js",
                  "input": "node_modules/@angular/common/locales/global",
                  "output": "/locale"
                },
                {
                  "glob": "CNAME",
                  "input": "surge",
                  "output": "/"
                },
                {
                  "glob": "index.html",
                  "input": "surge",
                  "output": "/"
                }
              ]
            },
            "simple": {
              "outputPath": "./simple2/client/",
              "index": "src/index.html",
              "assets": [
                {
                  "glob": "favicon.ico",
                  "input": "src",
                  "output": "/"
                },
                {
                  "glob": "images/**/*",
                  "input": "src/assets",
                  "output": "/assets"
                },
                {
                  "glob": "*",
                  "input": "configs",
                  "output": "/localdata"
                },
                {
                  "glob": "cr-en.js",
                  "input": "src/locale",
                  "output": "/locale"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "cricketereweb:build:serve"
          },
          "configurations": {
            "production": {
              "browserTarget": "cricketereweb:build:serveProduction"
            },
            "rtl": {
              "browserTarget": "cricketereweb:build:serveRtl"
            }
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "./host/server/ng",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json",
            "stylePreprocessorOptions": {
              "includePaths": [
                "./mockup/less",
                "./node_modules/shut/src/less"
              ]
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "media",
              "sourceMap": false,
              "namedChunks": true,
              "extractLicenses": true,
              "deleteOutputPath": false
            },
            "simple": {
              "outputPath": "./simple2/server",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "media",
              "sourceMap": false,
              "namedChunks": true,
              "extractLicenses": true,
              "deleteOutputPath": false
            }
          }
        },
        "pre": {
          "builder": "./builder-prerender:prerender",
          "options": {
            "browserTarget": "cricketereweb:build:production",
            "serverTarget": "cricketereweb:server:production",
            "routes": [
              "/projects/create",
              "/projects/1",
              "/content/details"
            ]
          }
        },
        "pre-multi": {
          "builder": "./builder-prerender:prerender-multi",
          "options": {
            "browserTarget": "cricketereweb:build:production",
            "serverTarget": "cricketereweb:server:production",
            "indexTarget": "cricketereweb:writeindex",
            "localePath": "locale",
            "routes": [
              "/projects/create",
              "/projects/1",
              "/content/details"
            ]
          }
        },
        "pre-simple": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "cricketereweb:build:simple",
            "serverTarget": "cricketereweb:server:simple",
            "guessRoutes": false,
            "routes": [
              "/projects/create",
              "/projects/1"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
